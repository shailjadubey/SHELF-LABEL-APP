	private static void No_btn() {
		// TODO Auto-generated method stub
		//first add barcode to list
		try
		{
			Thread.sleep(3000);
		driver.findElement(By.id("com.company.shelflabelsQA:id/etBarcodeSku")).sendKeys("13181831");
		Thread.sleep(3000);
		//open keyboard
		driver.findElement(By.id("com.company.shelflabelsQA:id/etBarcodeSku")).click();
		Thread.sleep(3000);
		//click on next button
		driver.executeScript("mobile:performEditorAction", ImmutableMap.of("action", "Next"));
		//click on add button
		Thread.sleep(3000);
		driver.findElement(By.id("com.company.shelflabelsQA:id/btnAdd")).click();
		Thread.sleep(3000);
		//Click on clear all button
		driver.findElement(By.id("com.company.shelflabelsQA:id/btnClealAll")).click();
		//check no button 
		Thread.sleep(3000);
		boolean no_btn=driver.findElement(By.id("com.company.shelflabelsQA:id/btnContinueToCancel")).isDisplayed();
		boolean expected=true;
		if(no_btn==expected)
		{
			System.out.println("Test case 47 Pass- No button successfully display on Confirmation popup ");
		}
		else
			System.out.println("Test Case 47 fail- No button not found on popup ");
		}
		catch(Exception e)
		{
			System.out.println("Test Case 47 fail- No button not found on popup ");
		}
	}

