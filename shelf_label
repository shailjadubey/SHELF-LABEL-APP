            print_screen_back();
            //Test case 57-Verify on Print screen , Print button should be enabled 
            print_screen_printbtn();
            //Test case 58-Verify Item Scanned should display on print screen
            Item_scanned();
}
	  
	private static void Item_scanned() {
		// TODO Auto-generated method stub
		try
		{
			Thread.sleep(3000);
			boolean itemscanned=driver.findElement(By.id("com.company.shelflabelsQA:id/tvNoOfItems")).isDisplayed();
			boolean expected=true;
			if(itemscanned==expected)
			{
				System.out.println("Test Case 58 Pass-Item scanned  successfully display on Print screen");
			}
			else
			{
				System.out.println("Test Case 58 Fail- Item scanned not display on Print screen ");
			}
			}	
			catch(Exception e)
			{
				System.out.println("Test Case 58 Fail- Item scanned not display on Print screen");
			}
	}

