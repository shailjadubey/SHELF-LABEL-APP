	//function to verify user is on Home page
	   private static void Home_test() throws InterruptedException {
		// TODO Auto-generated method stub
		  // boolean errorMsg_Actual= driver.findElement(By.xpath("(//android.widget.TextView[contains(text(),'Shelf Labels')])[1]")).isDisplayed();
	Thread.sleep(5000);
		   try
	{
		WebElement app_name_path=driver.findElement(By.className("android.widget.TextView"));
		Thread.sleep(4000);
		String app_name=app_name_path.getText();
		String Msg_Expected="Shelf Labels";
		System.out.println("App name is -"+app_name);
		Thread.sleep(2000);
		try
		{
			if(app_name.equals(Msg_Expected))
			{
				System.out.println("Test Case1 pass -User is on Home page 3");
			}
			else
			{
				System.out.println("Test Case1 fail -User is not on Home page");	
			}
		}
		catch (Error e)
		{
			System.out.println("Test Case1 fail -User is not on Home page");
		}	
		//System.out.println("Test case1 pass- User is on Home page");
	}
	 catch(Exception e)
	   {
		   System.out.println("Test case1 fail- User is not on Home page");
	   }
	
	   }
